{
  "name": "@unction/shuffle",
  "version": "2.63.0",
  "description": "Takes an Ordered Functor and returns an Ordered Functor with the same content, but in a random order",
  "keywords": [
    "unction",
    "functional"
  ],
  "contributors": [
    {
      "name": "Kurtis Rainbolt-Greene",
      "email": "kurtis@rainbolt-greene.online",
      "url": "https://kurtis.rainbolt-greene.online"
    }
  ],
  "license": "ISC",
  "homepage": "https://github.com/unctionjs/shuffle#readme",
  "repository": "github:unctionjs/shuffle",
  "bugs": "https://github.com/unctionjs/shuffle/issues",
  "main": "transpiled/index.js",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "NODE_ENV=test tap --100 './test.js'",
    "build": "babel './index.js' -d './transpiled/'",
    "lint": "eslint './index.js'",
    "check": "flow-typed install && flow check",
    "ci": "npm run test",
    "watch": "chokidar './index.js' './test.js' --command 'npm run ci --silent'"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-pipe-operator-curry": "1.0.2",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "6.23.0",
    "chokidar-cli": "1.2.0",
    "eslint": "3.19.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-promise": "3.5.0",
    "flow": "0.2.3",
    "flow-bin": "0.51.1",
    "flow-typed": "2.1.5",
    "nyc": "11.1.0",
    "tap": "10.3.4"
  },
  "dependencies": {
    "@unction/append": "2.13.0",
    "@unction/attach": "3.14.1",
    "@unction/exceptkey": "1.53.0",
    "@unction/fresh": "3.11.0",
    "@unction/last": "1.49.0",
    "@unction/length": "1.47.0",
    "@unction/mapvalues": "3.46.0",
    "@unction/range": "1.0.0",
    "@unction/reducevalues": "2.44.0"
  },
  "files": [
    "transpiled/index.js"
  ]
}
